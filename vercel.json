{
  "framework": "vite",
  "buildCommand": "npm install && npm run build",
  "installCommand": "npm install && npm rebuild esbuild rollup || true",
  "outputDirectory": "dist",
  "functions": {
    "nodeVersion": "20.x"
  },
  "build": {
    "env": {
      "ROLLUP_SKIP_NODE_PLATFORM_CHECK": "true",
      "ESBUILD_BINARY_PATH": "node_modules/esbuild/bin/esbuild"
    }
  }
}
